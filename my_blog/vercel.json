{
    "version": 2,
    "builds": [
        {
            "src": "my_blog/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "buildCommand": "bash build_files.sh"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "my_blog/wsgi.py"
        }
    ]
}